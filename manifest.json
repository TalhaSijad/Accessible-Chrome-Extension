{
    "manifest_version": 2,
    "name": "Accessibility Assistant",
    "description": "Designed for visually or hearing-impaired users.",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [ "activeTab" ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js object-src 'self'",
  }